{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "skipLibCheck": true,
    "outDir": "dist",
    "noEmit": false,
    "declaration": true,
    "lib": ["es2020"],
    "module": "ES2022",
    "moduleResolution": "node",
    "target": "es2022",
    "esModuleInterop": true,
    "types": ["astro/client"]
  },
  "include": ["./src/**/*.ts"]
}
